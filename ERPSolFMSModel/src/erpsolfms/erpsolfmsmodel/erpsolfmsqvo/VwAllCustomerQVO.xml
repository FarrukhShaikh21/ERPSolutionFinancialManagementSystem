<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="VwAllCustomerQVO"
  Version="12.1.3.10.47"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="erpsolfms.erpsolfmsmodel.erpsolfmsqvo.VwAllCustomerQVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P_ADF_USERID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P_ADF_MODULEID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P_ADF_LOCATIONID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P_ADF_REGIONID"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select loc.regionid,loc.locationid,loc.location_description,customerid,customer_name ,SUD.H_LEVEL 
from all_customers cus, all_locations loc,sys_users_detail sud 
where loc.locationid=cus.locationid 
and   sud.userid=:P_ADF_USERID 
and sud.moduleid=:P_ADF_MODULEID 
 AND  ((  sud.h_level ='C' and EXISTS (select '' from sys_user_customer suc where suc.customerid=cus.customerid and suc.userid=:P_ADF_USERID) ) 
 
     or 
     (sud.h_level='U' and loc.locationid=:P_ADF_LOCATIONID) 
     OR 
     (sud.h_level='R' and loc.REGIONID=:P_ADF_REGIONID) 
     OR 
     (sud.h_level='A' ) 
      
      )]]></SQLQuery>
  <ViewAttribute
    Name="Regionid"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="REGIONID"
    Expression="REGIONID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="3"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
